@model ControlVee.Port.InMan.DShop.Components.BatchModel

<li class="batch-partial-view list-group-item">
    <div class="batch-card">
            <div class="card-header card-title">
                <div class="prog-label col-form-label"></div>
                <div class="progress">
                    <div class="progress-bar" role="progressbar"></div>
                </div>
            </div>
      
        <dl class="row">
            <div class="card-body">
                <dt class="col-sm-2 card-header-pills badge-pill">
                    @Html.DisplayNameFor(model => model.NameOf)
                </dt>
                <dd class="col-sm-10 card-header-pills badge-pill">
                    @Html.DisplayFor(model => model.NameOf)
                </dd>
                <dt class="col-sm-2 card-header-pills badge-pill">
                    @Html.DisplayNameFor(model => model.ID)
                </dt>
                <dd class="col-sm-10 card-header-pills badge-pill">
                    @Html.DisplayFor(model => model.ID)
                </dd>
                <dt class="col-sm-2 card-header-pills badge-pill">
                    @Html.DisplayNameFor(model => model.Started)
                </dt>
                <dd class="col-sm-10 card-header-pills badge-pill">
                    @Html.DisplayFor(model => model.Started)
                </dd>
                <dt class="col-sm-2 card-header-pills badge-pill">
                    @Html.DisplayNameFor(model => model.Total)
                </dt>
                <dd class="col-sm-10 card-header-pills badge-pill">
                    @Html.DisplayFor(model => model.Total)
                </dd>
                <content>
                    <mark>
                        @Html.ActionLink("complete", "complete", new { /* id = Model.PrimaryKey */ })
                    </mark>
                </content>
            </div>
        </dl>
    </div>
</li>
